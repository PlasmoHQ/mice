{
  "$schema": "https://json.schemastore.org/chrome-manifest",
  "name": "Send vigorous racoon",
  "description": "Hello world",
  "version": "0.0.1",
  "author": "Plasmo Corp.",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/index.ts"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["activeTab"],
  "host_permissions": ["https://*/*"]
}
