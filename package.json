{
  "name": "mice",
  "displayName": "Mice",
  "version": "0.0.2",
  "description": "Open a hailing frequency via WebRTC for input sharing",
  "packageManager": "pnpm@6.32.8",
  "author": "L",
  "manifest": {
    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ],
    "host_permissions": [
      "https://*/*"
    ]
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": ">= 50%"
      }
    }
  },
  "scripts": {
    "start": "plasmo start",
    "dev": "plasmo dev",
    "build": "plasmo build",
    "test": "plasmo test",
    "web": "run-s web:*",
    "web:clean": "rimraf docs",
    "web:build": "parcel build .plasmo/static/options/index.html --dist-dir docs --no-source-maps --cache-dir .plasmo/cache/parcel-web --public-url https://plasmo-corp.github.io/mice/"
  },
  "dependencies": {
    "mnemonic-id": "3.2.7",
    "plasmo": "^0.18.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "simple-peer": "9.11.1"
  },
  "devDependencies": {
    "@plasmo-corp/rps": "1.2.1",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/chrome": "0.0.183",
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/simple-peer": "9.11.4",
    "buffer": "6.0.3",
    "events": "3.3.0",
    "parcel": "2.5.0",
    "prettier": "2.6.2",
    "process": "0.11.10",
    "rimraf": "3.0.2",
    "typescript": "4.6.4"
  }
}
